{
  "name": "@ttwtf/ng-parser",
  "version": "1.2.2",
  "description": "Advanced Angular parser with RAG/GraphRAG optimized output and extensible visitor architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ng-parser": "dist/bin/ng-parser.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!dist/**/__tests__/**",
    "!dist/src/__tests__/**",
    "README.md",
    "CHANGELOG.md",
    "CLI.md",
    "API_REFERENCE.md",
    "GETTING_STARTED.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:cli": "tsc && chmod +x dist/bin/ng-parser.js",
    "prepublishOnly": "npm run build:cli && npm test",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "angular",
    "parser",
    "ast",
    "cli",
    "rag",
    "graphrag",
    "llm",
    "knowledge-graph",
    "code-analysis",
    "visitor-pattern",
    "typescript",
    "static-analysis"
  ],
  "author": "Antoine Aire <antoine.aire@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apelleti/ng-parser.git"
  },
  "bugs": {
    "url": "https://github.com/apelleti/ng-parser/issues"
  },
  "homepage": "https://github.com/apelleti/ng-parser#readme",
  "dependencies": {
    "commander": "^14.0.1",
    "reflect-metadata": "^0.2.2",
    "simple-git": "^3.28.0",
    "typescript": ">=5.4.0 <5.7.0"
  },
  "peerDependencies": {
    "@angular/compiler": ">=18.0.0 <21.0.0",
    "@angular/compiler-cli": ">=18.0.0 <21.0.0",
    "@angular/core": ">=18.0.0 <21.0.0"
  },
  "devDependencies": {
    "@angular/compiler": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
